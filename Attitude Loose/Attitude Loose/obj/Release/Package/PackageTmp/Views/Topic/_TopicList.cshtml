@model IEnumerable<Attitude_Loose.ViewModels.TopicListViewModel>
@{
    ViewBag.Title = "_Topiclist";
}


@foreach (var item in Model)
{ <tr>
        <td class="span1">
            <i class="icon-star"></i>
        </td>
        <td class="span9">
            <div>@Html.ActionLink(item.TopicName, "Index", "Topic", new { id = item.TopicId }, null) </div>
            @*<div><small>@item.StartDate- @item.EndDate</small></div>*@
            <div><small> Created By @Html.ActionLink(item.UserName, "UserProfile", "ApplicationUser", new { id = item.UserId }, null)on @item.CreatedDate</small></div>
        </td>
        <td class="span2">
            <strong>@item.SupportsCount</strong> Supporters
        </td>
    </tr>

}





